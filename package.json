{
	"name": "vl-ui",
	"version": "0.2.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "^3.3.7",
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@monaco-digital/vl-types": "^1.7.3",
		"@reduxjs/toolkit": "^1.4.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/jest": "^26.0.14",
		"@types/node": "^14.11.8",
		"@types/react": "^16.9.52",
		"@types/react-dom": "^16.9.8",
		"@types/react-modal": "^3.12.0",
		"@types/react-redux": "^7.1.9",
		"axios": "^0.21.0",
		"chokidar-cli": "^2.1.0",
		"classnames": "^2.2.6",
		"graphql": "^15.5.0",
		"lint-staged": "^10.5.0",
		"lodash": "^4.17.20",
		"nanoid": "^3.1.20",
		"npm-run-all": "^4.1.5",
		"postcss-extend": "^1.0.5",
		"prettier": "^2.1.2",
		"react": "^16.14.0",
		"react-beautiful-dnd": "^13.0.0",
		"react-dom": "^16.14.0",
		"react-editor-js": "^1.6.2",
		"react-ga": "^3.3.0",
		"react-gtm-module": "^2.0.11",
		"react-input-autosize": "^3.0.0",
		"react-markdown": "^5.0.3",
		"react-modal": "^3.12.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"react-window": "^1.8.5",
		"tailwindcss": "^1.9.6",
		"typescript": "^4.0.3"
	},
	"scripts": {
		"build:tailwind": "postcss src/client/styles/main.scss -o src/client/styles/main.output.css",
		"watch:tailwind": "chokidar 'client/src/**/*.css' 'src/**/*.scss' --ignore src/client/styles/main.output.css -c 'npm run build:tailwind'",
		"start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
		"start:react": "react-scripts start",
		"prebuild": "run-s build:tailwind",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy:dev": "REACT_APP_STAGE=dev && npm run build && amplify env checkout dev && amplify publish --yes",
		"deploy:prod": "REACT_APP_STAGE=prod && npm run build && amplify env checkout prod && amplify publish --yes"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/classnames": "^2.2.11",
		"@types/react-transition-group": "^4.4.0",
		"autoprefixer": "^9.8.6",
		"docx": "^5.3.0",
		"file-saver": "^2.0.2",
		"husky": "^4.3.0",
		"install": "^0.13.0",
		"npm": "^6.14.9",
		"postcss": "^8.1.8",
		"postcss-cli": "^7.1.1",
		"postcss-comment": "^2.0.0",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.3",
		"react-helmet-async": "^1.0.7",
		"react-toastify": "^6.2.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
