{
	"name": "vl-ui",
	"version": "0.2.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "^3.3.21",
		"@material-ui/core": "^4.12.2",
		"@material-ui/icons": "^4.11.2",
		"@mdi/font": "^5.9.55",
		"@mdi/js": "^5.9.55",
		"@mdi/react": "^1.5.0",
		"@reduxjs/toolkit": "^1.6.1",
		"@stripe/react-stripe-js": "^1.4.1",
		"@stripe/stripe-js": "^1.16.0",
		"@types/jest": "^26.0.24",
		"@types/node": "^16.4.3",
		"@types/react": "^17.0.14",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.18",
		"axios": "^0.21.0",
		"chokidar-cli": "^2.1.0",
		"classnames": "^2.3.1",
		"file-saver": "^2.0.5",
		"graphql": "^15.5.1",
		"lodash": "^4.17.20",
		"nanoid": "^3.1.23",
		"npm-run-all": "^4.1.5",
		"postcss-extend": "^1.0.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-editor-js": "^1.10.0",
		"react-ga": "^3.3.0",
		"react-gtm-module": "^2.0.11",
		"react-helmet-async": "^1.0.7",
		"react-hook-form": "^6.15.4",
		"react-input-autosize": "^3.0.0",
		"react-markdown": "^6.0.2",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"react-window": "^1.8.5",
		"redux-thunk": "^2.3.0",
		"tailwindcss": "^1.9.6",
		"typescript": "^4.3.5"
	},
	"scripts": {
		"build:tailwind": "postcss src/client/styles/main.scss -o src/client/styles/main.output.css",
		"watch:tailwind": "chokidar 'client/src/**/*.css' 'src/**/*.scss' --ignore src/client/styles/main.output.css -c 'npm run build:tailwind'",
		"start:react": "react-scripts start",
		"start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
		"prebuild": "run-s build:tailwind",
		"build": "react-scripts build",
		"lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy:dev": "REACT_APP_STAGE=dev && yarn build && amplify env checkout dev && amplify publish",
		"deploy:prod": "REACT_APP_STAGE=prod && yarn build && amplify env checkout prod && amplify publish",
		"postinstall": "husky install",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^12.1.4",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.1.9",
		"@types/classnames": "^2.3.1",
		"@types/lodash": "^4.14.171",
		"@types/react-transition-group": "^4.4.2",
		"@typescript-eslint/eslint-plugin": "^4.28.4",
		"autoprefixer": "^9.8.6",
		"axios-mock-adapter": "^1.19.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.31.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest-dom": "^3.9.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-testing-library": "^4.10.1",
		"history": "^4.10.1",
		"husky": "^7.0.1",
		"install": "^0.13.0",
		"jest-junit": "^12.2.0",
		"jest-localstorage-mock": "^2.4.12",
		"lint-staged": "^11.1.1",
		"npm": "^7.20.0",
		"postcss": "^8.1.8",
		"postcss-cli": "^7.1.1",
		"postcss-comment": "^2.0.0",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.3",
		"prettier": "^2.3.2",
		"redux-mock-store": "^1.5.4",
		"semantic-release": "^19.0.3"
	},
	"release": {
		"branches": [
			{
				"name": "master"
			},
			{
				"name": "dev",
				"prerelease": true
			}
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/github"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"src/*.{js,ts,jsx,tsx}": "eslint --cache --fix"
	}
}
